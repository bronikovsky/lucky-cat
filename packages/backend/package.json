{
  "name": "@luckycat/backend",
  "version": "0.0.1",
  "main": "./src/index.ts",
  "private": true,
  "scripts": {
    "start:development": "NODE_ENV=development nodemon --exec ts-node ./src/index.ts",
    "start:production": "yarn build && NODE_ENV=production node ./build",
    "knex:run": "NODE_ENV=development knex --knexfile src/modules/database/knexfile.cjs.ts",
    "migrate:make": "yarn knex:run migrate:make -x ts",
    "migrate:latest": "yarn knex:run migrate:latest",
    "migrate:rollback": "yarn knex:run migrate:rollback",
    "seed:make": "yarn knex:run seed:make -x ts",
    "seed:run": "yarn knex:run seed:run",
    "build": "tsc --project ./",
    "release": "/bin/bash scripts/release.sh",
    "eslint": "eslint ./src/**/*.{tsx,ts}",
    "test": "jest"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-http-proxy": "^1.6.2",
    "knex": "^0.95.2",
    "objection": "^2.2.15",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "ramda": "^0.27.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.11",
    "@types/express-fileupload": "^1.1.6",
    "@types/express-http-proxy": "^1.6.1",
    "@types/jest": "^26.0.22",
    "@types/knex": "^0.16.1",
    "@types/node": "^14.14.35",
    "@types/passport": "^1.0.6",
    "@types/passport-local": "^1.0.33",
    "@types/ramda": "^0.27.39",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.23.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "semver": "^7.3.5",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
