{
  "name": "@luckycat/frontend",
  "version": "0.0.1",
  "main": "src/index.ts",
  "private": true,
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "scripts": {
    "storybook": "start-storybook -p 6006 -s public",
    "release": "/bin/bash scripts/release.sh",
    "start:development": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eslint": "eslint ./src/**/*.{tsx,ts}",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@reduxjs/toolkit": "^1.5.0",
    "connected-react-router": "^6.8.0",
    "dayjs": "^1.10.4",
    "history": "^4.10.1",
    "local-storage": "^2.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.2.4",
    "@storybook/addon-essentials": "^6.2.4",
    "@storybook/addon-links": "^6.2.4",
    "@storybook/node-logger": "^6.2.4",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.2.4",
    "@types/local-storage": "^1.4.0",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-select": "^4.0.13",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "react-scripts": "^4.0.1",
    "sass": "^1.32.8",
    "typescript": "~3.7.2"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  }
}
